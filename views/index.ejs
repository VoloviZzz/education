    <div class="s-item stat-back"><i class="fa fa-chevron-circle-up" aria-hidden="true"></i></div>

    <!-- стрелки -->
    <div class="strings">
      <div class="string-control">
        <div class="switch">
        	<label for="toggle"><i></i></label>
        	<span>синтаксический разбор</span>
        </div>
      </div>
      <div class="string-block">

      </div>
    </div>
    <div class="block-user-statistic">

      <div class="title_total">всего</div>
      <div class="title_text">синтаксич. | морфемный&nbsp;&nbsp;&nbsp;</div>
      <div class="count_analised_words"><span>0</span><span>0</span></div>
      <div class="title_errors">кол-во ошибки</div>
      <div class="count_errors"><span>0</span></div>
      <div class="title_error_avg">ошибки среднее</div>
      <div class="mean_count_errors"><span>0</span></div>
    </div>
    <div class="other_game">
      <a onclick="alert(test.getGreeting('market://details?id=com.elmsoftware.madtaxi.qpmdjm'));" class="my_game">
        <img src="img/madtaxi.png" alt="MadTaxi">
      </a>
      <a onclick="alert(test.getGreeting('market://details?id=com.elmsoftware.polyomino'));" class="my_game">
        <img src="img/polyomino.png" alt="MadTaxi">
      </a>
      <a onclick="alert(test.getGreeting('market://details?id=com.elmsoftware.gorescript'));" class="my_game">
        <img src="img/logo_alpha.png" alt="MadTaxi">
      </a>
    </div>
  </div>
  <div class="mode-select" style="display:none">
    <div class="select-block sintacs">
        <span>синтаксический разбор</span>
        <img src="img/note.png" alt="">
    </div>
    <div class="select-block sostav">
        <span>Морфемный разбор</span>
        <img src="img/beta.png" alt="">
    </div>
    <div class="select-block competition">
        <span>Добавление</span>
        <img src="img/medal.png" alt="">
    </div>
  </div>
  <div class="interface">
    <div class="i-item start">
      <img src="/img/cat/read_alpha.gif" class="cat read" alt="">
      <i class="fa fa-play-circle-o" aria-hidden="true"></i></div>
    <div class="i-item stat"><i class="fa fa-chevron-circle-down" aria-hidden="true"></i></div>
    <div class="i-item next"><i class="fa fa-arrow-circle-o-right" aria-hidden="true"></i></div>
    <div class="i-item home"><i class="fa fa-home" aria-hidden="true"></i></div>
    <div class="i-item song"><i class="fa fa-volume-up" aria-hidden="true"></i></div>
    <div class="i-item bulb"><i class="fa fa-lightbulb-o" aria-hidden="true"></i><div class="score">0</div></div>
    <div class="stars">
      <div class="star1"><i class="fa fa-star-o" aria-hidden="true"></i></div>
      <div class="star2"><i class="fa fa-star-o" aria-hidden="true"></i></div>
      <div class="star3"><i class="fa fa-star-o" aria-hidden="true"></i></div>
    </div>
    <div class="timer">
      0:00
    </div>
    <div class="figures">
      <div class="pristavka"></div>
      <div class="koren"></div>
      <div class="sufics"></div>
      <div class="okonchanie"></div>
      <div class="obratochka"></div>
      <div class="osnova"></div>
    </div>
    <div class="part_word">
      <div data-part="1" class="ppart1">Приставка</div>
      <div data-part="2" class="ppart2">Корень</div>
      <div data-part="3" class="ppart4">суффикс</div>
      <div data-part="4" class="ppart3">окончание</div>
      <div data-part="5" class="ppart5">постфикс</div>
    </div>
    <div class="part">
      <div data-part="1" class="part1">Подлежащее</div>
      <div data-part="2" class="part2">Сказуемое</div>
      <div data-part="4" class="part3">Определение</div>
      <div data-part="5" class="part4">Дополнение</div>
      <div data-part="3" class="part5">Обстоятельство</div>
    </div>
  </div>
  <script src="js/jquery-3.2.1.min.js"></script>
